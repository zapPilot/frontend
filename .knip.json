{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "workspaces": {
    ".": {
      "entry": ["src/app/**/*.{ts,tsx}", "*.config.{js,ts,mjs}"],
      "project": ["src/**/*.{ts,tsx}"]
    }
  },
  "ignore": ["**/*.test.{ts,tsx}", "**/*.spec.{ts,tsx}", "tests/**/*"],
  "ignoreExports": {
    "src/schemas/api/accountSchemas.ts": [
      "User",
      "UserCryptoWallet",
      "Plan",
      "UserSubscription",
      "ConnectWalletResponse",
      "AddWalletResponse",
      "UpdateEmailResponse",
      "UserProfileResponse",
      "AccountToken",
      "HealthCheckResponse",
      "MessageResponse"
    ],
    "src/schemas/api/analyticsSchemas.ts": [
      "ProtocolYieldToday"
    ],
    "src/schemas/api/balanceSchemas.ts": [
      "TokenBalanceRaw",
      "NormalizedTokenBalance",
      "WalletResponseData",
      "WalletTokenBalances"
    ],
    "src/services/analyticsService.ts": [
      "DailyYieldReturnsResponse",
      "PoolPerformanceResponse",
      "ProtocolYieldToday"
    ]
  }
}
